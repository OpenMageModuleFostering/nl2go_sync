<?xml version="1.0"?>
<!--

/**
 * NL2go newsletters subscribers list API extension
 *
 * @category   NL2go
 * @package    NL2go_Sync
 * @author     Anatoly V Koval
 */
 
-->
<config>
	<modules>
		<NL2go_Sync>
			<version>1.0.0</version>
		</NL2go_Sync>
	</modules>
	<global>
		<models>
			<sync>
				<rewrite>
					<newsletter_api_v2>NL2go_Sync_Model_Newsletter_Api_V2</newsletter_api_v2>
				</rewrite>
			</sync>
		</models>
		<helpers>
			<sync>
				<class>NL2go_Sync_Helper</class>
			</sync>
		</helpers>
	</global>
</config>
